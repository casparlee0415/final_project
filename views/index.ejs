<!DOCTYPE html>
<html>
    <head>
        <link rel = "stylesheet" href = "css/main.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <script src="js/main.js"></script>
    </head>
    <body>
        <div>
            <table>
                <thead>
                    <tr>
                        <th>品牌</th>
                    </tr>
                </thead>
                <tbody>
                    <% for(var i = 0;i < results.length;i++){ %>
                    <tr>
                        <td>
                            <% var form_id = 'form'+(i+1) %>
                            <form id=form_id action='/brand' method='POST'>
                                <input type="hidden" id="<%= results[i].brand_name %>" name="brand_name" value="<%= results[i].brand_name %>">
                                <a href="javascript:void(0)" onclick="parentNode.submit();"><%= results[i].brand_name %></a>
                            </form>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </body>
</html>